{
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "reload": true
  },
  "launcher": {
    "port": 8080,
    "startup_timeout_sec": 5.0,
    "open_browser": true
  },
  "data": {
    "root": "./data",
    "allow_abs_paths": true,
    "scan_cache_sec": 2.0,
    "max_scan_depth": -1,
    "max_upload_mb": 0
  },
  "logging": {
    "level": "INFO",
    "dir": "./logs"
  }
}
